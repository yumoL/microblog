{
  "apps": {
    "name": "Microblog",
    "script": "bin/www",
    "watch": true,
    "ignore_watch": [
      "node_modules",
      "logs",
      "uploadedFiles"
    ],
    "instances": 4,
    "error_file": "./logs/err.log",
    "out_file": "./logs/out.log",
    "log_date_format": "MM-DD-YYYY HH:mm:ss"
  }
}